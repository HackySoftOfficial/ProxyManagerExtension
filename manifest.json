{
  "manifest_version": 3,
  "name": "Simple Proxy Manager",
  "version": "1.0",
  "description": "A Chrome extension to manage proxy settings",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
